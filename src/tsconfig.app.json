{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [],
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "@sb-shared/*": [
        "../src/app/shared/*"
      ],
      "@sb-store/*": [
        "../src/app/store/*"
      ],
      "@sb-services/*": [
        "../src/app/services/*"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}